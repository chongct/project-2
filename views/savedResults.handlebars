<div class="tabs">
  <ul>
    <li class="is-active analyze"><a>Analyzed Results</a></li>
    <li class="search"><a>Search Results</a></li>
  </ul>
</div>

<div class="analyzedresult-container">
  {{#each analyzedList}}
    <div id="{{_id}}">
      <div class="result-title is-size-3">
        {{item}}
        <a class="delete delete-analyzed" data-id="{{_id}}"></a>
      </div>

      <div class="grid-container">
        {{#each result}}
          <div>
            {{#each sortedList}}
              <p>{{name}}</p>
              <p>{{address1}}</p>
              <p>{{address2}}</p>
              <br>
            {{/each}}
            {{range}}
          </div>
        {{/each}}
      </div>
    </div>
  {{/each}}
</div>

<div class="searchresult-container">
  {{#each searchList}}
    <div id="{{_id}}">
      <div class="result-title is-size-3">
        {{item}}
        <a class="delete delete-search" data-id="{{_id}}"></a>
      </div>

      <div class="grid-container">
        {{#each result}}
          <div>
            <a href={{link}}><p>{{name}}</p></a>
            <p>{{address1}}</p>
            <p>{{address2}}</p>
          </div>
        {{/each}}
      </div>
    </div>
  {{/each}}
</div>
